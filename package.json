{
	"name": "exp",
	"version": "0.3.4",
	"description": "Instant project forking via macOS APFS clonefile",
	"type": "module",
	"bin": {
		"exp": "./dist/exp.js"
	},
	"scripts": {
		"build": "bun build src/exp.ts --outfile dist/exp.js --target bun",
		"build:binary": "bun build src/exp.ts --compile --outfile dist/exp",
		"dev": "bun run src/exp.ts",
		"test": "bun test",
		"typecheck": "tsc --noEmit",
		"lint": "biome check .",
		"lint:fix": "biome check --write ."
	},
	"dependencies": {
		"@inquirer/prompts": "^7.10.0",
		"chalk": "^5.3.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.0",
		"@types/bun": "latest",
		"@types/node": "^22.0.0",
		"typescript": "^5.7.0"
	},
	"keywords": ["cli", "fork", "apfs", "clone", "dev-tools"],
	"author": "Joel Brubaker",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/thebrubaker/exp.git"
	},
	"homepage": "https://thebrubaker.github.io/exp/",
	"bugs": {
		"url": "https://github.com/thebrubaker/exp/issues"
	}
}
